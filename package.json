{
  "name": "schema-validator-agent",
  "version": "1.5.1",
  "type": "module",
  "main": "main.js",
  "description": "Automated schema validator for testing URLs against Schema.org and Google Rich Results Test",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "start-local": "node scripts/local-server.js",
    "validate": "node scripts/schema-validator.js",
    "validate-csv": "node scripts/csv-schema-validator.js",
    "enhance": "node scripts/schema-enhancer.js",
    "test": "node scripts/schema-validator.js",
    "build": "electron-packager . SchemaTools --platform=win32 --arch=x64 --out=dist --overwrite",
    "build:web": "echo 'Skipping Electron build for web deployment' || exit 0"
  },
  "keywords": [
    "schema",
    "validation",
    "seo",
    "schema.org",
    "google-rich-results"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "googleapis": "^144.0.0",
    "puppeteer": "^21.5.0"
  },
  "devDependencies": {
    "electron": "^39.1.0",
    "electron-packager": "^17.1.2"
  }
}
